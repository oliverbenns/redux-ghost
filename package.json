{
  "name": "redux-ghost",
  "version": "0.1.0",
  "description": "Redux state handler and api wrapper for Ghost Blog",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliverbenns/redux-ghost"
  },
  "scripts": {
    "build": "babel src --out-dir build",
    "clean": "rimraf dist build",
    "lint": "eslint src",
    "example": "npm --prefix ./example install && npm --prefix ./example start",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "redux",
    "react-redux",
    "redux-ghost",
    "ghost",
    "blog"
  ],
  "author": "Oliver Benns <info@oliverbenns.com> (http://github.com/oliverbenns)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oliverbenns/redux-ghost/issues"
  },
  "homepage": "https://github.com/oliverbenns/redux-ghost",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "react": "^16.2.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-redux": "^4.3.0 || ^5.0.0-beta",
    "redux": "^3.0.0",
    "redux-thunk": "^2.1.0"
  },
  "files": [
    "README.md",
    "build",
    "dist"
  ],
  "npmName": "redux-ghost",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
